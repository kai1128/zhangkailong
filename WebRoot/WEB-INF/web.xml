<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ServletA</servlet-name>
    <servlet-class>Servlet.ServletA</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ChengGong</servlet-name>
    <servlet-class>Servlet.ChengGong</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShiBai</servlet-name>
    <servlet-class>Servlet.ShiBai</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DengLu</servlet-name>
    <servlet-class>Servlet.DengLu</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ChengG</servlet-name>
    <servlet-class>Servlet.ChengG</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>chenggong</servlet-name>
    <servlet-class>Servlet.chenggong</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>ServletA</servlet-name>
    <url-pattern>/A</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChengGong</servlet-name>
    <url-pattern>/cg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShiBai</servlet-name>
    <url-pattern>/shibai</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DengLu</servlet-name>
    <url-pattern>/dl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChengG</servlet-name>
    <url-pattern>/CG</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>chenggong</servlet-name>
    <url-pattern>/chenggong</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
